		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/postcss": "^4.1.14",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"rollup-plugin-visualizer": "^5.12.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	}
